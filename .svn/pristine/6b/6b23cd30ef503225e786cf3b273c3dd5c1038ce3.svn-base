	document.getElementById('whyHellob01').style.display='none';
}


function whyLowing(){        
	var whyCorrect = 0;          
	for(var lowQnum = 1; lowQnum <= 10; lowQnum++){
		var whyLowradios = document.getElementsByName('whyLowquiz'+lowQnum);
		for(var j = 0; j < whyLowradios.length; j++) {
			var radio = whyLowradios[j];
			if(radio.value == "correct" && radio.checked) {
				whyCorrect++;
			}
		}
	}
	document.getElementById('whyScroe').innerHTML = "Low Test 결과 :<br> 총 "+whyCorrect+"문제 정답 ("+whyCorrect*10+"점)";
}


